{
  "name": "sentinelbot",
  "version": "1.0.0",
  "description": "AI personal assistant with multi-platform messaging, browser automation, and memory persistence",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "dev:cli": "tsx src/index.ts cli",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "assistant",
    "telegram",
    "discord",
    "automation",
    "agent",
    "multi-agent",
    "workflow",
    "browser-automation",
    "personal-assistant",
    "openai-compatible",
    "claude",
    "ollama"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/sentinelbot.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/sentinelbot/issues"
  },
  "homepage": "https://github.com/yourusername/sentinelbot#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "@prisma/client": "^5.10.0",
    "better-sqlite3": "^12.6.2",
    "bullmq": "^5.1.0",
    "chalk": "^5.3.0",
    "discord.js": "^14.14.0",
    "dotenv": "^16.4.0",
    "grammy": "^1.21.0",
    "ioredis": "^5.3.0",
    "js-yaml": "^4.1.0",
    "openai": "^4.28.0",
    "playwright": "^1.42.0",
    "uuid": "^9.0.0",
    "ws": "^8.16.0",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0",
    "prisma": "^5.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
}
